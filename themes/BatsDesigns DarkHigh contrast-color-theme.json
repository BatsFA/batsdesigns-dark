{
	"name": "Custom Dark Theme",
	"colors": {
		// --Main Editor--
		"editor.background": "#000000",
		"editor.foreground": "#dfdfdf",
		"editorLineNumber.foreground": "#11999e",
		"editorCursor.foreground": "#11999e",
		"textLink.foreground": "#11999e",
		"focusBorder": "#11999e",
		"editorOverviewRuler.border": "#000",
		
		// editor tabs
		"editorGroupHeader.tabsBackground": "#000000",
		"tab.activeBackground": "#0e0e0e",
		"tab.inactiveBackground": "#000000",
		"tab.hoverBorder": "#11999e",
		"tab.inactiveForeground": "#11999e",

		// --SideBar and ActivityBar--
		"sideBar.background": "#080808",
		"sideBar.foreground": "#dfdfdf",
		"sideBar.border": "#11999e00",
		"sideBarSectionHeader.background": "#080808",
		// Itemslist
		"list.hoverBackground": "#020202",
		"list.activeSelectionBackground": "#020202",
		"list.inactiveSelectionBackground": "#020202",
		"list.activeSelectionForeground": "#11999e",
		"list.inactiveSelectionForeground": "#11999e",
		"list.hoverForeground": "#fff",
		
		// buttons
		"button.background": "#11999e",
		"button.foreground": "#000000",
		// inputfields
		"input.background": "#000",
		// notifications like "1 unsaved"
		"badge.background": "#000009",
		// activitybar
		"activityBar.background": "#000000",
		"activityBar.foreground": "#dfdfdf",
		"activityBar.border": "#11999e",
		"activityBarBadge.background": "#000000",
		"activityBarBadge.foreground": "#fff",
		"activityBar.inactiveForeground": "#11999e",

		// --Top/Title NavBar--
		"titleBar.activeBackground": "#000",
		"titleBar.activeForeground": "#dfdfdf",
		"titleBar.inactiveBackground": "#080808",
		"titleBar.inactiveForeground": "#11999e",
		// Menu
		"menu.background": "#080808",
		"menu.foreground": "#fff",
		"menu.border": "#11999e",
		"menubar.selectionForeground": "#11999e",
		"menu.selectionBackground": "#000",
		"menubar.selectionBorder": "#11999e",

		// --Panel for the Terminal enz...--
		"panel.background": "#080808",
		"panel.border": "#11999e",
		"panelTitle.inactiveForeground": "#11999e",
		"panelTitle.activeForeground": "#FFFFFF",
		"panelTitle.activeBorder": "#11999e",
		// terminal
		"terminal.foreground": "#dfdfdf",
		"terminalCursor.background": "#ffffff",
    	"terminalCursor.foreground": "#11999e",
		"terminal.ansiYellow": "#e5e510",
		"terminal.ansiBrightYellow": "#ffff55",
		"terminal.ansiBlack": "#000000",
		"terminal.ansiBrightBlack": "#4e4e4e",
		"terminal.ansiBlue": "#0052aa",
		"terminal.ansiBrightBlue": "#3b8eea",
		"terminal.ansiCyan": "#11a8cd",
		"terminal.ansiBrightCyan": "#29b8db",
		"terminal.ansiGreen": "#00be9f",
		"terminal.ansiBrightGreen": "#00ffd5",
		"terminal.ansiMagenta": "#bc3fbc",
		"terminal.ansiBrightMagenta": "#d670d6",
		"terminal.ansiRed": "#cd3131",
		"terminal.ansiBrightRed": "#f14c4c",
		"terminal.ansiWhite": "#e5e5e5",
		"terminal.ansiBrightWhite": "#ffffff",
		// --statusbar on bottom--
		"statusBar.foreground": "#fff",
    	"statusBar.background": "#0c0c0d",
		"statusBarItem.hoverBackground": "#11999e",
		"statusBar.debuggingBackground": "#11999e",
		"statusBar.noFolderBackground": "#000000",
		
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#006090"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#EEFFFF"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#ff0000"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#11999e"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#00d7d9"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#00acff"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#F78C6C"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#FFCB6B"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
